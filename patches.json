        "removertl-cssmin": {
            "version": {
                "from": "1.1.70",
                "do": ""
            },
            "match": [
                "}\\[dir=ltr\\]\\s?([.a-zA-Z\\d[_]+?,\\[dir=ltr\\])",
                "}\\[dir=ltr\\]\\s?",
                "html\\[dir=ltr\\]",
                ",\\s?\\[dir=rtl\\].+?(\\{.+?\\})",
                "[\\w\\-\\.]+\\[dir=rtl\\].+?\\{.+?\\}",
                "\\}\\[lang=ar\\].+?\\{.+?\\}",
                "\\}\\[dir=rtl\\].+?\\{.+?\\}",
                "\\}html\\[dir=rtl\\].+?\\{.+?\\}",
                "\\}html\\[lang=ar\\].+?\\{.+?\\}",
                "\\[lang=ar\\].+?\\{.+?\\}",
                "html\\[dir=rtl\\].+?\\{.+?\\}",
                "html\\[lang=ar\\].+?\\{.+?\\}",
                "\\[dir=rtl\\].+?\\{.+?\\}",
                "\\[dir=str\\]",
                "[/]\\*([^*]|[\r\n]|(\\*([^/]|[\r\n])))*\\*[/]",
                "[/][/]#\\s.*",
                "\r?\n(?!\\(1|\\d)"
            ],
            "replace": [
                "}[dir=str] $1",
                "} ",
                "html",
                "$1",
                "",
                "}",
                "}",
                "}",
                "}",
                "",
                "",
                "",
                "",
                "[dir=ltr]",
                "",
                "",
                ""
            ]
        },
